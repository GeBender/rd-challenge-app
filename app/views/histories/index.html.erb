        <section id="main-content">
          <section class="wrapper">
            <!-- page start-->
            <div class="row">
                <div class="col-lg-12">
                    <!--breadcrumbs start -->
                    <ul class="breadcrumb">
                        <li><a href="/"><i class="fa fa-home"></i> Home</a></li>
                        <li><a href="/contacts"><i class="fa fa-users"></i> Contacts</a></li>
                        <li class="active"><i class="fa fa-list"></i> History</li>
                    </ul>
                    <!--breadcrumbs end -->
                </div>
            </div>

            <p id="notice"><%= notice %></p>

            <div class="row">
               <div class="col-lg-12">
                  <div id="accordion" class="panel-group m-bot20">
                      <div class="panel panel-default">
                          <div class="panel-heading">
                              <h4 class="panel-title">
                                  <a href="#collapseOne" data-parent="#accordion" data-toggle="collapse" class="accordion-toggle">
                                      Contact History <%= 'from ' + @contact.email if !@contact.email.nil? %>
                                  </a>
                              </h4>
                          </div>
                          <div class="panel-collapse collapse in" id="collapseOne">
                              <div class="panel-body">
                                <% if @contact.histories.count > 0 %>
                                <table class="table table-striped table-bordered">
                                  <thead>
                                    <tr>
                                      <th>Id</th>
                                      <th>Url</th>
                                      <th>Data</th>
                                      <th>Hora</th>
                                    </tr>
                                  </thead>

                                  <tbody>
                                    <% @contact.histories.each do |history| %>
                                      <tr>
                                        <td><%= @contact.id %></td>
                                        <td><%= history.url %></td>
                                        <td><%= history.created_at.strftime('%d/%m/%Y') %></td>
                                        <td><%= history.created_at.strftime('%H:%M') %></td>
                                      </tr>
                                    <% end %>
                                  </tbody>
                                </table>
                                <% else %>
                                  <div class="alert alert-warning" role="alert" style="text-align: center">
                                    <h4>Have you seen some BROWSING around here?</h4>
                                    <h2>:-(</h2>
                                  </div>
                                <% end %>
                              </div>
                          </div>
                      </div>
                  </div>

              <%#= link_to 'New Contact', new_contact_path %>
            </div>
          </section>
        </section>

<%#= link_to 'New History', new_history_path %>
