        <section id="main-content">
          <section class="wrapper">
            <!-- page start-->
            <div class="row">
                <div class="col-lg-12">
                    <!--breadcrumbs start -->
                    <ul class="breadcrumb">
                        <li><a href="/"><i class="fa fa-home"></i> Home</a></li>
                        <li class="active"><i class="fa fa-users"></i> Contacts</li>
                    </ul>
                    <!--breadcrumbs end -->
                </div>
            </div>

            <p id="notice"><%= notice %></p>

            <div class="row">
               <div class="col-lg-12">
                  <div id="accordion" class="panel-group m-bot20">
                      <div class="panel panel-default">
                          <div class="panel-heading">
                              <h4 class="panel-title">
                                  <a href="#collapseOne" data-parent="#accordion" data-toggle="collapse" class="accordion-toggle">
                                      Contacts
                                  </a>
                              </h4>
                          </div>
                          <div class="panel-collapse collapse in" id="collapseOne">
                              <div class="panel-body">
                                <table class="table table-striped table-bordered">
                                  <thead>
                                    <tr>
                                      <th>Name</th>
                                      <th>E-mail</th>
                                      <th>Pages</th>
                                      <th>Lest ping</th>
                                      <th></th>
                                    </tr>
                                  </thead>

                                  <tbody>
                                    <% @contacts.each do |contact| %>
                                      <tr>
                                        <td><%= contact.name %></td>
                                        <td><%= contact.email %></td>
                                        <td><%= contact.histories.count %></td>
                                        <td><%= contact.updated_at.strftime('%d/%m/%Y') %> at <%= contact.updated_at.strftime('%H:%M') %></td>
                                        <td><a href="/contact-history/<%= contact.id %>" class='btn btn-primary btn-sm'><i class="fa fa-list"></i> List pages</button></td>
                                      </tr>
                                    <% end %>
                                  </tbody>
                                </table>

                              </div>
                          </div>
                      </div>
                  </div>

              <%#= link_to 'New Contact', new_contact_path %>
            </div>
          </section>
        </section>

